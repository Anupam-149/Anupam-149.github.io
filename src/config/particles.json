        "enable": true,
        "minimumValue": 0.3
      },
      "value": {
        "min": 0.3,
        "max": 0.5
      },
      "animation": {
        "speed": 1,
        "minimumValue": 0.1
      }
    },
    "shape": {
      "options": {
        "polygon": {
          "sides": 6
        },
        "star": {
          "sides": 6
        }
      },
      "type": "polygon"
    },
    "size": {
      "random": {
        "enable": true,
        "minimumValue": 100
      },
      "value": {
        "min": 100,
        "max": 160
      },
      "animation": {
        "minimumValue": 40
      }
    }
